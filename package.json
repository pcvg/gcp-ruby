{
  "name": "gcp-ruby",
  "version": "1.0.0",
  "description": "Helper tools for gcp-ruby",
  "dependencies": {
    "dockerlint": "^0.3.9"
  },
  "devDependencies": {},
  "scripts": {
    "test": "dockerlint Dockerfile",
    "build": "docker build -t gcp-ruby/test -f ./Dockerfile .",
    "run": "docker run --rm -it --entrypoint sh gcp-ruby/test:latest",
    "clean": "docker image rm gcp-ruby/test:latest",
    "full-test": "npm run test && npm run build && npm run run && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pcvg/gcp-ruby.git"
  },
  "keywords": [
    "ruby",
    "gcp",
    "dockerfile",
    "dockerlint"
  ],
  "author": "Ain Tohvri <ain.tohvri@savings-united.com> (https://github.com/ain)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/pcvg/gcp-ruby/issues"
  },
  "homepage": "https://github.com/pcvg/gcp-ruby#readme"
}
